package com.luxoft.characterservice

interface CharacterService {
  version {major 1 minor 0}

  method ArmUp {
    in {
      Arm arm
    }
    out {
      String result
    }
  }

  method ArmDown {
    in {
      Arm arm
    }
    out {
      String result
    }
  }

  enumeration Arm { 
    LEFT 
    RIGHT 
  }
}
